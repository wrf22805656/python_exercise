import math

def polygon(t, n, length):
    angle = 360.0 / n
    for i in range(n):
        fd(t, length)
        lt(t, angle)
def circle(t,r):
    circumference = 2 * math.pi * r
    n = 50
    length = circumference / n
    (t,n,length)

print(circle(3,8))
